{
    "id": "uv",
    "version": "1.0.0",
    "name": "uv (Python Package Manager)",
    "description": "Installs uv, an extremely fast Python package and project manager",
    "documentationURL": "https://docs.astral.sh/uv/",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "0.4.18",
                "0.4.17"
            ],
            "default": "latest",
            "description": "Version of uv to install"
        },
        "preCommit": {
            "type": "string",
            "proposals": [
                "latest",
                "4.0.1",
                "3.8.0"
            ],
            "default": "latest",
            "description": "Version of pre-commit to install as uv tool (use 'false' to disable)"
        }
    },
    "installsAfter": []
}
